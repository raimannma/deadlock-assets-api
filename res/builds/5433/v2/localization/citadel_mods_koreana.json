      "upgrade_ability_power_shard_active": "\uad81\uadf9\uae30\ub97c \uc81c\uc678\ud55c \uac00\uc7a5 \ucd5c\uadfc\uc5d0 \uc0ac\uc6a9\ud55c \ub2a5\ub825\uc758 <span class=\"highlight\">\ucfe8\ub2e4\uc6b4\uc744 \ucd08\uae30\ud654</span>\ud569\ub2c8\ub2e4.",
      "upgrade_ability_refresher_desc": "\ubaa8\ub4e0 \ub2a5\ub825\uc758 <span class=\"highlight\">\ucfe8\ub2e4\uc6b4\uc774 \ucd08\uae30\ud654</span>\ub418\uace0 <span class=\"highlight\">\ubaa8\ub4e0 \ucda9\uc804\uc774 \ubcf5\uc6d0</span>\ub429\ub2c8\ub2e4.",
      "upgrade_acolytes_glove_desc": "\uc801\uad70 \uc601\uc6c5\uc5d0\uac8c <span class=\"highlight\">\uc57d\ud55c \ub610\ub294 \uac15\ud55c \uadfc\uac70\ub9ac</span> \uacf5\uaca9 \uc2dc \ucd94\uac00 <span class=\"highlight\">\ub9c8\ubc95 \ud53c\ud574</span>\ub97c \uc785\ud788\uace0 \ub300\uc0c1\uc758 <span class=\"highlight\">\ub9c8\ubc95 \uc800\ud56d</span>\uc744 \uac10\uc18c\uc2dc\ud0b5\ub2c8\ub2e4.",
      "upgrade_active_reload/buff": "\uae09\uc18d \uc7ac\uc7a5\uc804",
      "upgrade_active_reload_desc": "\uc7a5\uc804 \uc911 \uae09\uc18d \uc7ac\uc7a5\uc804 \uad6c\uac04\uc5d0\uc11c [{s:key_reload}] \ud0a4\ub97c \ub204\ub974\uba74 <span class=\"highlight\">\uc7ac\uc7a5\uc804\uc774 \uc989\uc2dc \uc644\ub8cc</span>\ub418\uace0 <span class=\"highlight\">\uc5f0\uc0ac \uc18d\ub3c4</span>\uc640 <span class=\"highlight\">\ucd1d\ud0c4 \uccb4\ub825 \ud761\uc218</span>\uac00 \ubd80\uc5ec\ub429\ub2c8\ub2e4.",
      "upgrade_ammo_scavenger_desc": "\uc601\ud63c\uc744 \ud68d\ub4dd\ud558\uac70\ub098 \ub514\ub098\uc774\ud560 \ub54c\ub9c8\ub2e4 <span class=\"highlight\">\ud0c4\uc57d\uc744 \ub3cc\ub824\ubc1b\uace0</span> <span class=\"highlight\">\ub9c8\ub825</span> \uc911\ucca9\uc744 \uc5bb\uc2b5\ub2c8\ub2e4.<br>\ucd5c\ub300 \uc911\ucca9 \uc2dc \uc9c8\uc8fc \uc18d\ub3c4 \ubcf4\ub108\uc2a4\ub97c \uc5bb\uc2b5\ub2c8\ub2e4.",
      "upgrade_arcane_extension_desc": "\ub2a5\ub825\uacfc \uc544\uc774\ud15c\uc758 <span class=\"highlight\">\uc9c0\uc18d \uc2dc\uac04</span>\uc774 \uc99d\uac00\ud569\ub2c8\ub2e4.",
      "upgrade_arcane_surge_desc": "<span class=\"highlight\">\ub300\uc2dc \uc810\ud504</span> \uc2dc \ucd94\uac00 \ud65c\ub825\uc744 \uc18c\ubaa8\ud558\uc9c0 \uc54a\uc73c\uba70 \uc774\ud6c4 <span class=\"highlight\">\ub2a5\ub825 \uc0ac\uc6a9 \uc2dc</span> \ubcf4\ub108\uc2a4 <span class=\"highlight\">\ubc94\uc704, \uc9c0\uc18d \uc2dc\uac04</span>, <span class=\"highlight\">\ub9c8\ub825</span>\uc744 \uc5bb\uc2b5\ub2c8\ub2e4. \ucd94\uac00\ub85c \ub2e4\uc74c \uc7ac\uc7a5\uc804\uae4c\uc9c0 <span class=\"highlight\">\uc5f0\uc0ac \uc18d\ub3c4</span> \ubc0f \ubcf4\ub108\uc2a4 <span class=\"highlight\">\ud0c4\uc57d</span>\uc744 \uc5bb\uc2b5\ub2c8\ub2e4. \ucd5c\ub300 {s:AbilityDuration}\ucd08 \ub3d9\uc548 \uc9c0\uc18d\ub429\ub2c8\ub2e4.",
      "upgrade_banshee_slugs_headshots_desc": "<span class=\"highlight\">\ud5e4\ub4dc\uc0f7</span>\uc73c\ub85c \uba85\uc911\ud558\uba74 \ub300\uc0c1\uc758 <span class=\"highlight\">\ucd1d\ud0c4\uacfc \ub9c8\ubc95 \uc800\ud56d</span>\uc774 \uac10\uc18c\ud569\ub2c8\ub2e4.",
      "upgrade_berserker_desc": "\uc9c0\uc18d \ud53c\ud574\ub97c \ubc1b\uc744\uc218\ub85d <span class=\"highlight\">\ubb34\uae30 \ud53c\ud574</span>\uac00 \uc99d\uac00\ud569\ub2c8\ub2e4.",
      "upgrade_boxing_glove_desc": "<span class=\"highlight\">\uadfc\uac70\ub9ac \uacf5\uaca9</span> \uc2dc \ub300\uc0c1\uc758 <span class=\"highlight\">\uc774\ub3d9 \uc18d\ub3c4\uac00 \ub454\ud654</span>\ub418\uace0 \ub300\uc0c1\uc5d0 \uc785\ud788\ub294 <span class=\"highlight\">\uadfc\uac70\ub9ac \ud53c\ud574</span>\uc758 {s:LifestealHealPercent}% \ubc0f \ucd94\uac00 {s:LifestealHeal}\ub9cc\ud07c \uccb4\ub825\uc774 <span class=\"highlight\">\uce58\uc720</span>\ub429\ub2c8\ub2e4. \ube44\uc601\uc6c5 \uc0c1\ub300 \uc2dc \ucd1d\uce58\uc720\ub7c9\uc758 {s:NonHeroHealPct}%\uac00 \uce58\uc720\ub429\ub2c8\ub2e4.",
      "upgrade_bullet_armor_reduction_aura_desc": "\uadfc\ucc98 \uc801\uad70\uc758 <span class=\"highlight\">\ucd1d\ud0c4 \uc800\ud56d \ubc0f \uc5f0\uc0ac \uc18d\ub3c4</span>\uac00 \uac10\uc18c\ud569\ub2c8\ub2e4. \uadfc\ucc98\uc5d0 \uc801\uad70 \uc601\uc6c5\uc774 \ub2e8 \ud55c \uba85\ub9cc \uc788\ub294 \uacbd\uc6b0, <span class=\"highlight\">\ud6a8\uacfc\uac00 3\ubc30</span>\ub85c \uc99d\uac00\ud569\ub2c8\ub2e4.",
      "upgrade_bullet_resist_shredder/modifier_bullet_armor_shredder_proc/modifier_bullet_armor_shredder": "\ucd1d\ud0c4 \ubc29\uc5b4 \uac10\uc18c",
      "upgrade_bullet_resist_shredder_desc": "\uc801\uad70\uc5d0\uac8c <span class=\"highlight\">\ub9c8\ubc95 \ud53c\ud574</span>\ub97c \uc785\ud788\uba74 \uc801\uad70\uc758 <span class=\"highlight\">\ucd1d\ud0c4 \uc800\ud56d</span>\uc774 \uac10\uc18c\ud569\ub2c8\ub2e4.",
      "upgrade_burst_fire/modifier_burst_fire_buff": "\uae09\uc18d \uc0ac\uaca9",
      "upgrade_burst_fire_desc": "\ucd1d\ud0c4\uc774 1\ubc1c\uc774\ub77c\ub3c4 \uc801\uad70 \uc601\uc6c5\uc5d0\uac8c \uc801\uc911\ud558\uba74 \uc7a0\uc2dc \ub3d9\uc548 <span class=\"highlight\">\uc5f0\uc0ac \uc18d\ub3c4</span>\uc640 <span class=\"highlight\">\uc774\ub3d9 \uc18d\ub3c4</span>\uac00 \uc99d\uac00\ud569\ub2c8\ub2e4.",
      "upgrade_cardio_calibrator_desc": "\uc790\uc2e0\uc5d0\uac8c \uc801\uc6a9\ub41c <span class=\"highlight\">\uc774\ub3d9 \uc18d\ub3c4 \ub454\ud654</span> \ud6a8\uacfc\uac00 \uac10\uc18c\ud569\ub2c8\ub2e4.",
      "upgrade_chain_lightning_desc": "\ub300\uc0c1\uc774 \ucd1d\ud0c4\uc5d0 \ub9de\uc73c\uba74 \uc77c\uc815 \ud655\ub960\ub85c \ub300\uc0c1\uc5d0 <span class=\"highlight\">\uc804\uae30 \ucda9\uaca9</span> \ud53c\ud574\ub97c \uc785\ud799\ub2c8\ub2e4. <span class=\"highlight\">\uc804\uae30 \ucda9\uaca9</span>\uc740 \uadfc\ucc98 \uc801\uad70\uc5d0\uac8c \uc5f0\uc1c4 \ud53c\ud574\ub97c \uc785\ud799\ub2c8\ub2e4.",
      "upgrade_chonky_desc": "<span class=\"highlight\">{s:RestoreDelay}\ucd08</span>\ub3d9\uc548 \ud53c\ud574\ub97c \uc785\uc9c0 \uc54a\uc73c\uba74 <span class=\"highlight\">\uccb4\ub825 \uc7ac\uc0dd\uc774 \ucd5c\ub300 {s:HealLifePercentOutOfCombat}%</span>\ub85c \uc99d\uac00\ud569\ub2c8\ub2e4.",
      "upgrade_cloaking_device_active_ambush_desc": "<br>\uacf5\uaca9\ud558\uac70\ub098 \ub2a5\ub825\uc744 \uc2dc\uc804\ud558\uc5ec \uc740\uc2e0 \uc0c1\ud0dc\uac00 \ud574\uc81c\ub418\uba74 <span class=\"highlight\">\uae30\uc2b5</span>\uc774 \uc2dc\uc791\ub429\ub2c8\ub2e4. \uc77c\uc2dc\uc801\uc73c\ub85c \ubcf4\ub108\uc2a4 <span class=\"highlight\">\uc5f0\uc0ac \uc18d\ub3c4</span>\uc640 <span class=\"highlight\">\ub9c8\ub825</span>\uc774 \ubd80\uc5ec\ub429\ub2c8\ub2e4.",
      "upgrade_cloaking_device_active_desc": "<span class=\"highlight\">\uc740\uc2e0</span> \uc0c1\ud0dc\uac00 \ub429\ub2c8\ub2e4. \uc740\uc2e0 \uc0c1\ud0dc\uc5d0\uc11c \ud53c\ud574\ub97c \uc785\uc73c\uba74 \uc9e7\uc740 \uc2dc\uac04 \ub3d9\uc548 \ubaa8\uc2b5\uc774 \ub178\ucd9c\ub429\ub2c8\ub2e4.",
      "upgrade_close_quarter_combat/modifier_cqc_proc/modifier_slow_base": "\ub454\ud654",
      "upgrade_close_quarter_combat_desc": "<span class=\"highlight\">\uadfc\uac70\ub9ac</span>\uc5d0 \uc788\ub294 \ub300\uc0c1\uc5d0 \ucd94\uac00 <span class=\"highlight\">\ubb34\uae30 \ud53c\ud574</span>\ub97c \uc785\ud788\uace0 \ub300\uc0c1\uc774 \ucd1d\ud0c4\uc5d0 \ub9de\uc73c\uba74 <span class=\"highlight\">\uc774\ub3d9 \uc18d\ub3c4\uac00 \ub454\ud654</span>\ub429\ub2c8\ub2e4.",
      "upgrade_close_range_desc": "<span class=\"highlight\">\uadfc\uac70\ub9ac</span>\uc5d0 \uc788\ub294 \ub300\uc0c1\uc5d0 \ucd94\uac00 <span class=\"highlight\">\ubb34\uae30 \ud53c\ud574</span>\ub97c \uc785\ud799\ub2c8\ub2e4.",
      "upgrade_cold_front_desc": "\uc5bc\uc74c \ud3ed\ud48d\uc774 \ud655\uc7a5\ud558\uba70 \ub300\uc0c1\uc5d0 <span class=\"highlight\">\ub9c8\ubc95 \ud53c\ud574</span>\ub97c \uc785\ud788\uace0 <span class=\"highlight\">\uc774\ub3d9 \uc18d\ub3c4\ub97c \ub454\ud654</span>\ud569\ub2c8\ub2e4. \ube44\uc601\uc6c5 \ub300\uc0c1\uc774 \uc785\ub294 \ud53c\ud574\ub294 {s:NPCDamageMult}\ubc30\ub85c \uc99d\uac00\ud569\ub2c8\ub2e4.",
      "upgrade_containment_desc": "\ub300\uc0c1\uc5d0 <span class=\"highlight\">\ub9c8\ubc95 \ud53c\ud574</span>\ub97c \uc785\ud788\uace0 \uc774\ub3d9\uacfc \ub300\uc2dc \uc18d\ub3c4\ub97c <span class=\"highlight\">\ub454\ud654</span>\ud569\ub2c8\ub2e4. \ub610\ud55c <span class=\"highlight\">\uc774\ub3d9 \uae30\ubc18 \uc544\uc774\ud15c\uacfc \ub2a5\ub825\uc5d0 \uce68\ubb35</span>\uc744 \uc801\uc6a9\ud569\ub2c8\ub2e4.<br><span class=\"diminish\">\ub300\uc0c1\uc758 \ud65c\ub825 \uc0ac\uc6a9\uc5d0\ub294 \uc601\ud5a5\uc744 \ubbf8\uce58\uc9c0 \uc54a\uc2b5\ub2c8\ub2e4.</span>",
      "upgrade_cooldown_reduction_desc": "\ub2a5\ub825\uacfc \uc544\uc774\ud15c\uc758 <span class=\"highlight\">\ucfe8\ub2e4\uc6b4</span>\uc774 \uac10\uc18c\ud569\ub2c8\ub2e4.",
      "upgrade_crackshot_desc": "\ucd1d\ud0c4\uc73c\ub85c \ubcf4\ub108\uc2a4 <span class=\"highlight\">\ub9c8\ubc95 \ud53c\ud574</span>\ub97c \uc785\ud799\ub2c8\ub2e4.",
      "upgrade_damage_recycler_desc": "\uc801\uad70\uc774 \uc801\uc6a9\ud55c <span class=\"highlight\">\uce58\uc720 \uac10\uc18c</span> \ud6a8\uacfc\uac00 \uac10\uc18c\ud569\ub2c8\ub2e4.",
      "upgrade_debuff_reducer_desc": "\uc790\uc2e0\uc5d0\uac8c \uc801\uc6a9\ub41c \ubaa8\ub4e0 \ubd80\uc815\uc801\uc778 \ud6a8\uacfc\uc758 <span class=\"highlight\">\uc9c0\uc18d \uc2dc\uac04</span>\uc774 \uac10\uc18c\ud569\ub2c8\ub2e4.",
      "upgrade_diviners_kevlar_desc": "<span class=\"highlight\">\uad81\uadf9\uae30</span> \uc2dc\uc804 \uc2dc \uc77c\uc2dc\uc801\uc73c\ub85c <span class=\"highlight\">\ucd1d\ud0c4 \ubc29\uc5b4\ub9c9, \ub9c8\ubc95 \ubc29\uc5b4\ub9c9</span>, <span class=\"highlight\">\ub9c8\ub825</span>\uc744 \uc5bb\uc2b5\ub2c8\ub2e4.",
      "upgrade_dps_aura_active_desc": "\uc790\uc2e0\uacfc \uadfc\ucc98 \uc544\uad70\uc5d0\uac8c \ubcf4\ub108\uc2a4 <span class=\"highlight\">\uc774\ub3d9 \uc18d\ub3c4</span>\uc640 <span class=\"highlight\">\uc5f0\uc0ac \uc18d\ub3c4</span>\ub97c \ubd80\uc5ec\ud569\ub2c8\ub2e4.",
      "upgrade_dps_aura_desc": "\uadfc\ucc98 \ud50c\ub808\uc774\uc5b4 \ubbf8\ub2c8\uc5b8\uc758 <span class=\"highlight\">\uc5f0\uc0ac \uc18d\ub3c4</span>\ub97c \uc99d\uac00\ud569\ub2c8\ub2e4.",
      "upgrade_escalating_exposure/modifier_proc_watcher/modifier_debuff": "\ub9c8\ubc95 \uc99d\ud3ed \uc911\ucca9",
      "upgrade_escalating_exposure_desc": "\uc801\uad70\uc5d0\uac8c <span class=\"highlight\">\ub9c8\ubc95 \ud53c\ud574</span>\ub97c \uc785\ud788\uba74 <span class=\"highlight\">\ub9c8\ubc95 \uc99d\ud3ed</span>\uc774 \uc911\ucca9\ub418\uc5b4 \uc801\uad70\uc774 \uc785\ub294 <span class=\"highlight\">\ub9c8\ubc95 \ud53c\ud574</span>\uac00 \uc99d\uac00\ud569\ub2c8\ub2e4.",
      "upgrade_fervor_passive": "<span class=\"highlight\">\uccb4\ub825\uc774 {s:LowHealthThreshold}% \ubbf8\ub9cc</span>\uc77c \ub54c \ub2a5\ub825\uce58 \ubcf4\ub108\uc2a4\ub97c \uc5bb\uc2b5\ub2c8\ub2e4.",
      "upgrade_glass_cannon_desc": "\uc601\uc6c5\uc744 \ucc98\uce58\ud560 \ub54c\ub9c8\ub2e4 <span class=\"highlight\">\uc601\uad6c \ubb34\uae30 \ud53c\ud574</span>(\ucd5c\ub300 \uc911\ucca9 {s:MaxStacks}\ud68c)\ub97c \uc5bb\uc2b5\ub2c8\ub2e4. \uc0ac\ub9dd \uc2dc \uc911\ucca9 1\ud68c\uac00 \uc0ac\ub77c\uc9d1\ub2c8\ub2e4.",
      "upgrade_glitch/modifier_glitch_debuff": "\uc800\uc8fc \uc0c1\ud0dc!",
      "upgrade_glitch_desc": "\uc801\uad70\uc5d0\uac8c \uc800\uc8fc\ub97c \uac78\uc5b4 <span class=\"highlight\">\uc2dc\uc804 \ubc29\ud574, \uce68\ubb35, \ubb34\uc7a5 \ud574\uc81c</span> \uc0c1\ud0dc\ub85c \ub9cc\ub4e4\uace0 <span class=\"highlight\">\uc544\uc774\ud15c \uc0ac\uc6a9\uc744 \ucc28\ub2e8</span>\ud569\ub2c8\ub2e4.",
      "upgrade_headshot_booster_desc": "<span class=\"highlight\">\ud5e4\ub4dc\uc0f7</span> \uc801\uc911 \uc2dc \ubcf4\ub108\uc2a4 <span class=\"highlight\">\ubb34\uae30 \ud53c\ud574</span>\ub97c \uc785\ud799\ub2c8\ub2e4.",
      "upgrade_heal_on_level/heal_on_level_buff": "\ub808\ubca8\ubcc4 \uce58\uc720",
      "upgrade_heal_on_level_buff": "\ub808\ubca8\ubcc4 \uce58\uc720",
      "upgrade_heal_on_level_desc": "<span class=\"highlight\">\uc740\ucd1d</span>\uc744 \ubc1b\uc73c\uba74 \uc790\ub3d9\uc73c\ub85c <span class=\"highlight\">\uce58\uc720</span>\ub429\ub2c8\ub2e4.",
      "upgrade_healbane_desc": "\uc801\uad70\uc5d0\uac8c <span class=\"highlight\">\ub9c8\ubc95 \ud53c\ud574</span>\ub97c \uc785\ud788\uba74 <span class=\"highlight\">\uce58\uc720 \uac10\uc18c</span> \ud6a8\uacfc\uac00 \uc801\uc6a9\ub429\ub2c8\ub2e4. \uc774 \ud6a8\uacfc\uac00 \uc801\uc6a9\ub41c \uc801\uad70 \uc601\uc6c5\uc774 \uc0ac\ub9dd\ud560 \uacbd\uc6b0, \uc790\uc2e0\uc758 \uccb4\ub825\uc774 \ud06c\uac8c \uce58\uc720\ub429\ub2c8\ub2e4.",
      "upgrade_healing_booster_desc": "<span class=\"highlight\">\uce58\uc720</span>\uac00 <span class=\"highlight\">{s:HealAmpRegenPercent}%</span> \uadf8\ub9ac\uace0 <span class=\"highlight\">\uce58\uc720 \uac10\uc18c \uc800\ud56d</span>\uc774 <span class=\"highlight\">{s:DegenResistance}%</span> \uc99d\uac00\ud569\ub2c8\ub2e4.",
      "upgrade_health_nova_desc": "\uc790\uc2e0\uacfc \uadfc\ucc98 \uc544\uad70\uc744 <span class=\"highlight\">\uce58\uc720</span>\ud569\ub2c8\ub2e4. \uc544\uad70 1\uba85\ub2f9 {s:HeroTargetBonus}%\uc529 \ucd94\uac00\ub85c \uce58\uc720\ud569\ub2c8\ub2e4.",
      "upgrade_health_stimpak/modifier_stimpak_regen": "\uce58\uc720 \uc758\uc2dd",
      "upgrade_health_stimpak_desc": "\ub300\uc0c1\uc5d0 <span class=\"highlight\">\uc7ac\uc0dd</span> \ubc0f <span class=\"highlight\">\uc9c8\uc8fc \uc18d\ub3c4</span>\uac00 \ubd80\uc5ec\ub429\ub2c8\ub2e4. \uc801\uad70 \uc601\uc6c5\uc774\ub098 \uc8fc\uc694 \ubaa9\ud45c\ubb3c\uc758 \uacf5\uaca9\uc73c\ub85c \ud53c\ud574\ub97c \uc785\uc73c\uba74 \ud6a8\uacfc\uac00 \uc911\ub2e8\ub429\ub2c8\ub2e4. \uc790\uc2e0\uc5d0\uac8c \uc2dc\uc804\ud560 \uc218 \uc788\uc2b5\ub2c8\ub2e4.",
      "upgrade_hemorrhage/modifier_item_hemorrhage/modifier_hemorrhage": "\ucd9c\ud608",
      "upgrade_hollow_point_rounds_desc": "<span class=\"highlight\">\uccb4\ub825\uc774 {s:LifeThreshold}% \uc774\uc0c1<span class=\"highlight\">\uc77c \ub54c \ucd94\uac00 <span class=\"highlight\">\ubb34\uae30 \ud53c\ud574</span>\ub97c \uc785\ud799\ub2c8\ub2e4.",
      "upgrade_imbued_duration_extender_desc": "\ub2a5\ub825\uacfc \uc544\uc774\ud15c\uc758 <span class=\"highlight\">\uc9c0\uc18d \uc2dc\uac04</span>\uc774 \uc99d\uac00\ud569\ub2c8\ub2e4.",
      "upgrade_infuser_desc": "<span class=\"highlight\">\ub9c8\ub825</span> \ubc0f <span class=\"highlight\">\ub9c8\ubc95 \uccb4\ub825 \ud761\uc218</span>\ub97c \uc5bb\uc2b5\ub2c8\ub2e4.",
      "upgrade_inhibitor_desc": "\ub300\uc0c1\uc774 \ucd1d\ud0c4\uc5d0 \ub9de\uc73c\uba74 <span class=\"highlight\">\uc774\ub3d9 \uc18d\ub3c4\uac00 \ub454\ud654</span>\ub418\uace0 \ub300\uc0c1\uc774 <span class=\"highlight\">\uc785\ud788\ub294 \ud53c\ud574</span>\uac00 \uac10\uc18c\ud569\ub2c8\ub2e4.",
      "upgrade_intensifying_clip_desc": "\ubb34\uae30\ub97c \uc5f0\uc0ac\ud560\uc218\ub85d <span class=\"highlight\">\ubb34\uae30 \ud53c\ud574</span>\uac00 \uc99d\uac00\ud569\ub2c8\ub2e4.",
      "upgrade_kinetic_sash_desc": "<span class=\"highlight\">\ub300\uc2dc \uc810\ud504</span> \uc2dc \ucd94\uac00 \ud65c\ub825\ub97c \uc18c\ubaa8\ud558\uc9c0 \uc54a\uc73c\uba70, \ub2e4\uc74c \uc7ac\uc7a5\uc804\uae4c\uc9c0 <span class=\"highlight\">\uc5f0\uc0ac \uc18d\ub3c4</span>\uc640 \ubcf4\ub108\uc2a4 <span class=\"highlight\">\ud0c4\uc57d</span>\uc744 \uc5bb\uc2b5\ub2c8\ub2e4. \ucd5c\ub300 {s:AbilityDuration}\ucd08 \ub3d9\uc548 \uc9c0\uc18d\ub429\ub2c8\ub2e4.",
      "upgrade_lifestrike_gauntlets_desc": "<span class=\"highlight\">\uadfc\uac70\ub9ac</span> \uacf5\uaca9 \uc2dc \ub300\uc0c1\uc5d0 \uc785\ud788\ub294 <span class=\"highlight\">\uadfc\uac70\ub9ac \ud53c\ud574</span>\uc758 {s:LifestrikeHealPercent}% \ubc0f \ucd94\uac00 {s:LifestrikeHeal}\ub9cc\ud07c \uccb4\ub825\uc774 <span class=\"highlight\">\uce58\uc720</span>\ub429\ub2c8\ub2e4. \ube44\uc601\uc6c5 \uc0c1\ub300 \uc2dc \ucd1d\uce58\uc720\ub7c9\uc758 {s:NonHeroHealPct}%\uac00 \uce58\uc720\ub429\ub2c8\ub2e4.",
      "upgrade_long_range_desc": "\ub300\uc0c1\uacfc\uc758 \uac70\ub9ac\uac00 <span class=\"highlight\">\ucd5c\uc18c \uac70\ub9ac \uc774\uc0c1</span>\uc778 \uacbd\uc6b0 \ucd94\uac00 <span class=\"highlight\">\ubb34\uae30 \ud53c\ud574</span>\ub97c \uc785\ud799\ub2c8\ub2e4.",
      "upgrade_long_range_slowing_tech/modifier_long_range_slowing_tech_proc/modifier_slow_base": "\ub454\ud654",
      "upgrade_magic_burst_desc": "\ub2a5\ub825\uc73c\ub85c \ud55c \ubc88\uc5d0 {s:MinimumDamage} \uc774\uc0c1\uc758 \ud53c\ud574\ub97c \uc785\ud788\uba74 \ubcf4\ub108\uc2a4 <span class=\"highlight\">\ub9c8\ubc95 \ud53c\ud574</span>\ub97c \uc785\ud799\ub2c8\ub2e4.",
      "upgrade_magic_carpet_desc": "\ub9c8\ubc95 \uc591\ud0c4\uc790\ub97c \uc18c\ud658\ud558\uba74 \ucd1d\ud0c4 \ubc0f \ub9c8\ubc95 \ubc29\uc5b4\ub9c9\uc744 \uc5bb\uc73c\uba70 \uc591\ud0c4\uc790\ub97c \ud0c0\uace0 \ube44\ud589\ud560 \uc218 \uc788\uc2b5\ub2c8\ub2e4. \uc591\ud0c4\uc790\ub97c \uc18c\ud658\ud558\ub294 \ub3d9\uc548\uc5d0\ub294 \ub2a5\ub825\uc744 \uc0ac\uc6a9\ud560 \uc218 \uc5c6\uc2b5\ub2c8\ub2e4. \ube44\ud589 \uc911\uc5d0\ub294 \ub454\ud654 \ud6a8\uacfc\uc5d0 \uba74\uc5ed\uc774 \ub418\uace0 \ub3d9\uc791\uc744 \ucde8\ud558\uba74 \uc591\ud0c4\uc790\uac00 \uc0ac\ub77c\uc9d1\ub2c8\ub2e4.",
      "upgrade_magic_reach_desc": "\ub2a5\ub825\uacfc \uc544\uc774\ud15c\uc758 <span class=\"highlight\">\uc2dc\uc804 \ubc94\uc704</span> \ubc0f <span class=\"highlight\">\ud6a8\uacfc \ubc18\uacbd</span>\uc774 \uc99d\uac00\ud569\ub2c8\ub2e4.",
      "upgrade_magic_shield_desc": "<span class=\"highlight\">\ub9c8\ubc95 \ubc29\uc5b4\ub9c9</span>\uc73c\ub85c \ubcf4\ud638\ubc1b\ub294 \ub3d9\uc548 \ubcf4\ub108\uc2a4 <span class=\"highlight\">\ub9c8\ub825</span> \ubc0f <span class=\"highlight\">\ucfe8\ub2e4\uc6b4 \uac10\uc18c</span>\ub97c \uc5bb\uc2b5\ub2c8\ub2e4.",
      "upgrade_magic_shock_desc": "\ub2a5\ub825\uc73c\ub85c \ud55c \ubc88\uc5d0 {s:MinimumDamage} \uc774\uc0c1\uc758 \ud53c\ud574\ub97c \uc785\ud788\uba74 \ubcf4\ub108\uc2a4 <span class=\"highlight\">\ub9c8\ubc95 \ud53c\ud574</span>\ub97c \uc785\ud799\ub2c8\ub2e4.<br/>\ud53c\ud574\ub97c \uc785\uc740 \ub300\uc0c1\uc740 <span class=\"highlight\">{s:ReProcLockoutTime}\ucd08</span> \ub3d9\uc548 \ud3ed\ubc1c \uac15\ud654 \ud6a8\uacfc\uc5d0 \uba74\uc5ed\uc774 \ub429\ub2c8\ub2e4.",
      "upgrade_magic_tempo_desc": "\ub2a5\ub825\uacfc \uc544\uc774\ud15c\uc758 <span class=\"highlight\">\ucfe8\ub2e4\uc6b4</span>\uc774 \uac10\uc18c\ud569\ub2c8\ub2e4.",
      "upgrade_magic_vulnerability_desc": "\ub300\uc0c1\uc774 <span class=\"highlight\">\ub9c8\ubc95 \ud53c\ud574</span>\ub97c \uc785\uc73c\uba74 <span class=\"highlight\">\ub9c8\ubc95 \uc800\ud56d</span>\uc774 \uac10\uc18c\ud569\ub2c8\ub2e4.",
      "upgrade_medic_bullets_desc": "\ucd1d\ud0c4\uc5d0 \ub9de\uc740 \ub300\uc0c1\uc5d0 \ub530\ub77c \uc790\uc2e0\uc744 <span class=\"highlight\">\uce58\uc720</span>\ud569\ub2c8\ub2e4.",
      "upgrade_melee_charge_desc": "\uc801\uad70\uc5d0\uac8c <span class=\"highlight\">\uac15\ud55c \uadfc\uac70\ub9ac \uacf5\uaca9</span> \uc2dc <span class=\"highlight\">\ud53c\ud574\uac00 \uc99d\uac00</span>\ud558\uace0 <span class=\"highlight\">\ubb34\uae30\uac00 \uc989\uc2dc \uc7ac\uc7a5\uc804</span>\ub418\uac70\ub098 \ubcf4\ub108\uc2a4 \ud0c4\uc57d <span class=\"highlight\">+100%</span>\ub97c \ud68d\ub4dd\ud569\ub2c8\ub2e4.<br>\ubcf4\ub108\uc2a4 \ud0c4\uc57d\uc774 \uac00\ub4dd \ucc2c \uc0c1\ud0dc\uac00 \uc544\ub2cc \uacbd\uc6b0\uc5d0\ub9cc \ud65c\uc131\ud654\ub429\ub2c8\ub2e4.",
      "upgrade_personal_rejuvenator_desc": "\uc601\uc6c5\uc774 \uc0ac\ub9dd\ud55c \uc790\ub9ac\uc5d0\uc11c \ubd80\ud65c\ud569\ub2c8\ub2e4.",
      "upgrade_pristine_emblem_desc": "\uc801\uad70\uc758 <span class=\"highlight\">\uccb4\ub825\uc774 {s:EnemyLifeThreshold}% \uc774\uc0c1<span class=\"highlight\">\uc77c \ub54c \ucd94\uac00 <span class=\"highlight\">\ubb34\uae30 \ud53c\ud574</span>\ub97c \uc785\ud799\ub2c8\ub2e4.",
      "upgrade_quick_silver_desc": "\ucc98\uc74c \ud0c0\uaca9 \uc2dc \ubcf4\ub108\uc2a4 <span class=\"highlight\">\ub9c8\ubc95 \ud53c\ud574</span>\ub97c \uc785\ud788\ub3c4\ub85d \ub2a5\ub825\uc744 \ubcf4\uac15\ud569\ub2c8\ub2e4. \ud574\ub2f9 \ub2a5\ub825\uc744 \uc0ac\uc6a9\ud558\uba74 \ubb34\uae30\uac00 \uc989\uc2dc <span class=\"highlight\">\uc7ac\uc7a5\uc804</span>\ub418\uace0 \ud574\ub2f9 \ud0c4\ucc3d\uc740 <span class=\"highlight\">\uc5f0\uc0ac \uc18d\ub3c4</span> \ubcf4\ub108\uc2a4\ub97c \uc5bb\uc2b5\ub2c8\ub2e4.<br>\ud0c4\uc57d\uc774 \uac00\ub4dd \ucc2c \uc0c1\ud0dc\uac00 \uc544\ub2cc \uacbd\uc6b0\uc5d0\ub9cc \ud65c\uc131\ud654\ub429\ub2c8\ub2e4.",
      "upgrade_rebirth_passive": "\uc0ac\ub9dd \uc2dc {s:RespawnDelay}\ucd08 \ud6c4 \uccb4\ub825 \uc77c\ubd80\ub97c \ud68c\ubcf5\ud55c \uc0c1\ud0dc\ub85c <span class=\"highlight\">\uc0ac\ub9dd\ud55c \uc790\ub9ac\uc5d0\uc11c \ubd80\ud65c</span>\ud569\ub2c8\ub2e4.",
      "upgrade_reduce_debuff_duration_desc": "\uc790\uc2e0\uc5d0\uac8c \uc801\uc6a9\ub41c \ubaa8\ub4e0 \ubd80\uc815\uc801\uc778 \ud6a8\uacfc\uc758 <span class=\"highlight\">\uc9c0\uc18d \uc2dc\uac04</span>\uc774 \uac10\uc18c\ud569\ub2c8\ub2e4.",
      "upgrade_regenerating_bullet_shield_desc": "<span class=\"highlight\">\ucd1d\ud0c4 \ubc29\uc5b4\ub9c9</span>\uc73c\ub85c \ubcf4\ud638\ubc1b\ub294 \ub3d9\uc548 <span class=\"highlight\">\ubb34\uae30 \ud53c\ud574</span> \ubc0f <span class=\"highlight\">\uc5f0\uc0ac \uc18d\ub3c4</span>\uac00 \uc99d\uac00\ud569\ub2c8\ub2e4.",
      "upgrade_reinforcing_casings_desc": "\ucd1d\ud0c4\uc73c\ub85c \uc801\uad70 \uc601\uc6c5\uc744 \uc801\uc911\ud558\uba74 <span class=\"highlight\">\ucd1d\ud0c4 \uc800\ud56d</span>\uc744 \uc5bb\uc2b5\ub2c8\ub2e4. <span class=\"highlight\">1\ubc1c\ub2f9 \uc911\ucca9 1\ud68c\ub9cc \uc5bb\uc744 \uc218 \uc788\uc2b5\ub2c8\ub2e4.</span>",
      "upgrade_rescue_beam_desc": "\ub300\uc0c1 \uc544\uad70 \uc601\uc6c5\uacfc \uc790\uc2e0\uc744 <span class=\"highlight\">\ucd5c\ub300 \uccb4\ub825</span>\uc758 \uc77c\uc815 \ube44\uc728\ub85c <span class=\"highlight\">\uce58\uc720</span>\ud569\ub2c8\ub2e4. \uce58\uc720 \uc911\uc5d0 <span class=\"highlight\">\ub300\uc0c1\uc744 \uc790\uc2e0 \ucabd\uc73c\ub85c \ub04c\uc5b4\ub2f9\uae38</span> \uc218 \uc788\uc2b5\ub2c8\ub2e4. \uc790\uc2e0\uc5d0\uac8c \uc2dc\uc804\ud560 \uc218 \uc788\uc2b5\ub2c8\ub2e4.",
      "upgrade_rescue_beam_pull": "\ub300\uc0c1 \ub04c\uc5b4\ub2f9\uae30\uae30",
      "upgrade_restorative_locket_active_desc": "\ubaa8\ub4e0 \uc911\ucca9\uc744 \uc18c\ubaa8\ud558\uc5ec <span class=\"highlight\">\ub300\uc0c1 \uc544\uad70\uc744 \uce58\uc720</span>\ud558\uace0 <span class=\"highlight\">\ud65c\ub825</span>\uc744 \ubcf4\ucda9\ud569\ub2c8\ub2e4. \uc790\uc2e0\uc5d0\uac8c \uc2dc\uc804\ud560 \uc218 \uc788\uc2b5\ub2c8\ub2e4.",
      "upgrade_restorative_locket_passive_desc": "{s:Radius}m \uc774\ub0b4\uc758 \uc801\uad70\uc774 \ub2a5\ub825\uc744 \uc0ac\uc6a9\ud558\uba74 <span class=\"highlight\">\ud68c\ubcf5 \uc911\ucca9</span>\uc774 1\ud68c \uc800\uc7a5\ub429\ub2c8\ub2e4.",
      "upgrade_return_fire_desc": "\uc801\uad70\uc774 \ub2a5\ub825 \ub610\ub294 \ubb34\uae30\ub85c \uacf5\uaca9\ud558\uc5ec \uc790\uc2e0\uc774 \ud53c\ud574\ub97c \uc785\uc73c\uba74 \uc790\ub3d9\uc73c\ub85c \ud574\ub2f9 \uc801\uad70\uc5d0\uac8c <span class=\"highlight\">\ucd1d\ud0c4\uc744 \ubc1c\uc0ac</span>\ud569\ub2c8\ub2e4.",
      "upgrade_ricochet_desc": "\ub300\uc0c1\uc774 \ucd1d\ud0c4\uc5d0 \ub9de\uc73c\uba74 \uadfc\ucc98 \uc801\uad70\uc5d0\uac8c <span class=\"highlight\">\ud295\uae30\uace0 </span> <span class=\"highlight\">\ubaa8\ub4e0 \ud655\ub960 \uae30\ubc18 \ucd1d\ud0c4 \ud6a8\uacfc</span>\uac00 \uc801\uc6a9\ub418\uba70 <span class=\"highlight\">\uae30\ubcf8 \ud53c\ud574\uc758 \uc77c\uc815 \ube44\uc728\ub85c \ud53c\ud574\ub97c \uc785\ud799\ub2c8\ub2e4.</span>",
      "upgrade_rocket_booster_desc": "\uacf5\uc911\uc73c\ub85c \ub192\uc774 <span class=\"highlight\">\ub0a0\uc544\uc624\ub985\ub2c8\ub2e4.</span> \uacf5\uc911\uc5d0 \ub5a0 \uc788\uc744 \ub54c \ub2a5\ub825\uc744 \ub2e4\uc2dc \uc0ac\uc6a9\ud558\uc5ec \ube60\ub974\uac8c \ub099\ud558\ud560 \uc218 \uc788\uc2b5\ub2c8\ub2e4.<br>\uc801\uad70 \uc601\uc6c5\uc5d0\uac8c \ud53c\ud574\ub97c \uc785\uc73c\uba74 {s:InteruptCooldown}\ucd08 \ub3d9\uc548 \uc0ac\uc6a9\ud560 \uc218 \uc5c6\uc2b5\ub2c8\ub2e4.",
      "upgrade_rupture/modifier_rupture": "\ubd80\ud328",
      "upgrade_rupture_desc": "\ub300\uc0c1 \uc801\uad70\uc774 \ubc1b\ub294 <span class=\"highlight\">\uce58\uc720</span>\uac00 \uac10\uc18c\ud558\uace0 \ud604\uc7ac \uccb4\ub825\uc758 \uc77c\uc815 \ube44\uc728\ub85c <span class=\"highlight\">\uc9c0\uc18d \ud53c\ud574</span>\ub97c \uc785\ud799\ub2c8\ub2e4.<br>\ubd80\ud328 \ud53c\ud574\ub85c \uc801\uad70\uc744 \ucc98\uce58\ud560 \uc218 \uc5c6\uc2b5\ub2c8\ub2e4.",
      "upgrade_sharpshooter_desc": "\ub300\uc0c1\uacfc\uc758 \uac70\ub9ac\uac00 <span class=\"highlight\">\ucd5c\uc18c \uac70\ub9ac \uc774\uc0c1</span>\uc778 \uacbd\uc6b0 \ucd94\uac00 <span class=\"highlight\">\ubb34\uae30 \ud53c\ud574</span>\ub97c \uc785\ud799\ub2c8\ub2e4.",
      "upgrade_silencer_active": "\ub300\uc0c1\uc774 \ucd1d\ud0c4\uc5d0 \ub9de\ub294 \uc989\uc2dc {s:AbilityDuration}\ucd08 \ub3d9\uc548 <span class=\"highlight\">\uce68\ubb35</span> \uc0c1\ud0dc\uac00 \ub429\ub2c8\ub2e4. \uce68\ubb35\uc740 \ub300\uc0c1\uc774 \ub2a5\ub825\uc744 \uc0ac\uc6a9\ud560 \uc218 \uc5c6\ub3c4\ub85d \ud569\ub2c8\ub2e4.",
      "upgrade_siphon_bullets_desc_passive2": "\ub300\uc0c1\uc774 \ucd1d\ud0c4\uc5d0 \ub9de\uc73c\uba74 \uc77c\uc2dc\uc801\uc73c\ub85c \uc801\uad70\uc758 <span class=\"highlight\">\ucd5c\ub300 \uccb4\ub825\uc744 \ud761\uc218</span>\ud569\ub2c8\ub2e4. \uc57d\ud654 \ud6a8\uacfc\uac00 \ub05d\ub098\uba74 \uc801\uad70\uc740 \ud761\uc218\ub2f9\ud55c \uccb4\ub825\uc744 \ud68c\ubcf5\ud569\ub2c8\ub2e4. \uccb4\ub825 \ud761\uc218 \ub300\uc0c1\uc774 \uc0ac\ub9dd\ud558\uba74 \uc601\uad6c\uc801\uc73c\ub85c \uccb4\ub825 {s:StealPerKill}\uc774 \uc911\ucca9\ub418\uace0, \uc790\uc2e0\uc774 \uc0ac\ub9dd\ud558\uba74 \uc911\ucca9 {s:StackLostPerDeath}\ud68c\ub97c \uc783\uc2b5\ub2c8\ub2e4.",
      "upgrade_slowing_bullets/modifier_slowing_bullets_proc/modifier_slow_base": "\ub454\ud654",
      "upgrade_slowing_bullets_desc": "\ub300\uc0c1\uc774 \ucd1d\ud0c4\uc5d0 \ub9de\uc73c\uba74 <span class=\"highlight\">\uc774\ub3d9 \uc18d\ub3c4\uac00 \ub454\ud654</span>\ub429\ub2c8\ub2e4.",
      "upgrade_slowing_tech/modifier_slowing_tech_proc/modifier_slow_base": "\ub454\ud654",
      "upgrade_spellshield/modifier_spellshield_buff": "\uc721\uacb9\uaf43\uc78e \ubcf4\ud638\uad6c",
      "upgrade_spellshield/modifier_spellshield_linger_buff": "\uc721\uacb9\uaf43\uc78e \ubcf4\ud638\uad6c \ud574\uc81c!",
      "upgrade_spellshield_desc": "<span class=\"highlight\">\ub9c8\ubc95 \ud53c\ud574</span> \ub610\ub294 <span class=\"highlight\">\uc57d\ud654</span> \ud6a8\uacfc\ub97c \ucc28\ub2e8\ud569\ub2c8\ub2e4. \uc804\ud22c\uc5d0\uc11c \ubc97\uc5b4\ub098\uc57c\ub9cc \uc7ac\uc0dd\ub429\ub2c8\ub2e4.",
      "upgrade_spellshield_shield": "\uc601\ud63c \ucc28\ub2e8 \ubb38\uc591",
      "upgrade_superacolytes_glove/modifier_superacoloytes_glove": "\ub108\ud074 \ubd09\uc778 \ud574\uc81c!",
      "upgrade_superacolytes_glove_desc": "\ucd95\uc801\ub41c <span class=\"highlight\">{s:StoredSpiritDamage}</span> \ub9c8\ubc95 \ud53c\ud574\ub97c \uc785\ud78c \ud6c4 <span class=\"highlight\">\uac15\ud55c \uadfc\uac70\ub9ac \uacf5\uaca9</span>\uc744 \uac00\ud558\uba74 \ucd94\uac00 <span class=\"highlight\">{s:StoredSpiritDamage} \ub9c8\ubc95 \ud53c\ud574</span>\ub97c \uc785\ud799\ub2c8\ub2e4.",
      "upgrade_suppressor_desc": "\uc801\uad70\uc5d0\uac8c <span class=\"highlight\">\ub9c8\ubc95 \ud53c\ud574</span>\ub97c \uc785\ud790 \uacbd\uc6b0 \ud53c\ud574\ub97c \uc785\uc740 \uc801\uad70\uc758 <span class=\"highlight\">\uc5f0\uc0ac \uc18d\ub3c4\uac00 \ub454\ud654</span>\ub429\ub2c8\ub2e4.",
      "upgrade_surging_power_active": "<span class=\"highlight\">\uccb4\ub825 \ud761\uc218</span>, <span class=\"highlight\">\uc5f0\uc0ac \uc18d\ub3c4</span>, <span class=\"highlight\">\ud0c4\uc57d</span>\uc774 \ubd80\uc5ec\ub429\ub2c8\ub2e4. \ucd94\uac00\ub41c \ud0c4\uc57d\uc740 \ucd5c\ub300 \ud0c4\ucc3d \ud06c\uae30\uc5d0 \uc81c\ud55c\ubc1b\uc9c0 \uc54a\uc2b5\ub2c8\ub2e4.",
      "upgrade_targeted_silence_desc": "\ucc98\uc74c \ud0c0\uaca9\ud55c \ub300\uc0c1\uc744 <span class=\"highlight\">\uce68\ubb35 \uc0c1\ud0dc\ub85c \ub9cc\ub4e4\uace0 \ud53c\ud574\ub97c \uc785\ud788\ub294</span> \ud22c\uc0ac\uccb4\ub97c \ubc1c\uc0ac\ud569\ub2c8\ub2e4. \uce68\ubb35\uc740 \ub2a5\ub825 \uc9d1\uc911\uc744 \ubc29\ud574\ud558\uc9c0 \uc54a\uc2b5\ub2c8\ub2e4.",
      "upgrade_tech_bleed_desc": "\ub2a5\ub825\uc73c\ub85c \ud53c\ud574\ub97c \uc785\uc740 \ub300\uc0c1\uc5d0 <span class=\"highlight\">\ub454\ud654</span> \ud6a8\uacfc\ub97c \uc801\uc6a9\ud558\ub3c4\ub85d \ub2a5\ub825\uc744 \ubcf4\uac15\ud569\ub2c8\ub2e4. \uc9e7\uc740 \uc9c0\uc5f0 \uc2dc\uac04 \ud6c4 \ub300\uc0c1\uc774 <span class=\"highlight\">\uc785\uc740 \ud53c\ud574\uc758 \uc77c\uc815 \ube44\uc728</span>\ub85c \uadfc\ucc98 \uc801\uad70\uc5d0\uac8c \ucd94\uac00 \ud53c\ud574\ub97c \uc785\ud799\ub2c8\ub2e4.",
      "upgrade_tech_damage_pulse_desc": "\uac00\uc7a5 \uac00\uae4c\uc774 \uc788\ub294 \uc801\uad70 2\uba85\uc5d0\uac8c \uc9c0\uc18d\uc801\uc73c\ub85c <span class=\"highlight\">\ub9c8\ubc95 \ud53c\ud574</span>\ub97c \uc785\ud799\ub2c8\ub2e4.",
      "upgrade_tech_defense_shredders_desc": "\ub300\uc0c1\uc774 \ucd1d\ud0c4\uc5d0 \ub9de\uc73c\uba74 \ub300\uc0c1\uc5d0 <span class=\"highlight\">\ub9c8\ubc95 \ud53c\ud574</span>\uac00 \uc99d\ud3ed\ub418\ub294 \uc57d\ud654 \ud6a8\uacfc\ub97c \uc801\uc6a9\ud558\uace0 \ud574\ub2f9 \ub300\uc0c1\uc5d0 \ub300\ud55c <span class=\"highlight\">\ub9c8\ubc95 \uccb4\ub825 \ud761\uc218</span>\uac00 \ubd80\uc5ec\ub429\ub2c8\ub2e4.",
      "upgrade_tech_overflow_desc": "\uc801\uad70 \uc601\uc6c5\uc774 \ucd1d\ud0c4\uc5d0 \ub9de\uc73c\uba74 \uac15\ud654 \ud6a8\uacfc\uac00 <span class=\"highlight\">\ucd95\uc801</span>\ub418\uba70 \uc644\uc804\ud788 \ucd95\uc801 \ud6c4 \ubcf4\ub108\uc2a4 <span class=\"highlight\">\uc5f0\uc0ac \uc18d\ub3c4</span>\uc640 <span class=\"highlight\">\ub9c8\ub825</span>\uc744 \uc5bb\uc2b5\ub2c8\ub2e4.",
      "upgrade_tech_range_desc": "\ub2a5\ub825\uacfc \uc544\uc774\ud15c\uc758 <span class=\"highlight\">\ubc94\uc704</span> \ubc0f <span class=\"highlight\">\ud6a8\uacfc \ubc18\uacbd</span>\uc774 \uc99d\uac00\ud569\ub2c8\ub2e4.",
      "upgrade_thermal_detonator_desc": "\uc811\ucd09 \uc2dc \ud3ed\ubc1c\ud558\ub294 \ud50c\ub77c\uc2a4\ud06c\ub97c \ub358\uc838 \ubc18\uacbd \ub0b4\uc5d0 \uc788\ub294 \ub300\uc0c1\uc5d0 \uc810\ucc28 \uc99d\uac00\ud558\ub294 <span class=\"highlight\">\ucd08\ub2f9 \ub9c8\ubc95 \ud53c\ud574</span>\ub97c \uc785\ud799\ub2c8\ub2e4. \uc544\uad70 \uc601\uc6c5\uc740 \ud574\ub2f9 \ub300\uc0c1\uc5d0 \ucd94\uac00 <span class=\"highlight\">\ubb34\uae30 \ud53c\ud574</span>\ub97c \uc785\ud799\ub2c8\ub2e4.<br><br>\ube44\uc601\uc6c5 \uc0c1\ub300 \uc2dc \ucd08\ub2f9 \ud53c\ud574\uac00{s:NonHeroReductionPercent}% \uac10\uc18c\ud569\ub2c8\ub2e4.",
      "upgrade_toxic_bullets_buildup": "\ub3c5\uc131 \ucd95\uc801 \uc911...",
      "upgrade_toxic_bullets_desc": "\ub300\uc0c1\uc774 \ucd1d\ud0c4\uc5d0 \ub9de\uc73c\uba74 \ub3c5\uc131\uc774 \ucd95\uc801\ub418\uba70 \uc644\uc804\ud788 \ucd95\uc801 \ud6c4 <span class=\"highlight\">\ucd5c\ub300 \uccb4\ub825</span>\uc758 <span class=\"highlight\">\uc77c\uc815 \ube44\uc728</span>\ub85c <span class=\"highlight\">\ucd9c\ud608 \ud53c\ud574</span>\ub97c \uc785\uc2b5\ub2c8\ub2e4. \ucd9c\ud608 \ud53c\ud574\ub97c \uc785\uc740 \ub300\uc0c1\uc740 <span class=\"highlight\">\uce58\uc720\uac00 \uac10\uc18c</span>\ub429\ub2c8\ub2e4.",
      "upgrade_unstoppable_desc": "\uc77c\uc2dc\uc801\uc73c\ub85c <span class=\"highlight\">\ubd80\uc815\uc801 \uc0c1\ud0dc \ud6a8\uacfc</span>\ub97c \uc5b5\uc81c\ud558\uace0 <span class=\"highlight\">\uae30\uc808, \uce68\ubb35, \uc218\uba74, \uc774\ub3d9 \ubd88\uac00, \ubb34\uc7a5 \ud574\uc81c</span>\uc5d0 <span class=\"highlight\">\uba74\uc5ed</span>\uc774 \ub429\ub2c8\ub2e4. <br><span class=\"highlight\">\uae30\uc808</span> \ub610\ub294 <span class=\"highlight\">\uc218\uba74</span> \uc0c1\ud0dc\uc5d0\uc11c\ub294 \uc0ac\uc6a9\ud560 \uc218 \uc5c6\uc2b5\ub2c8\ub2e4.",
      "upgrade_vex_barrier_desc": "<span class=\"highlight\">\uc774\ub3d9 \uc81c\ud55c, \uae30\uc808, \uc18d\ubc15, \uc774\ub3d9 \ubd88\uac00 \ub610\ub294 \uc218\uba74</span> \uc0c1\ud0dc\uc77c \ub54c \uc790\ub3d9\uc73c\ub85c <span class=\"highlight\">\ud65c\ub825\uc744 \ud68c\ubcf5</span>\ud558\uace0 \uc77c\uc2dc\uc801\uc778 <span class=\"highlight\">\ucd1d\ud0c4 \ubc0f \ub9c8\ubc95 \ubc29\uc5b4\ub9c9</span>\uc774 \ud65c\uc131\ud654\ub429\ub2c8\ub2e4.",
      "upgrade_vex_barrier_shield": "\ubc18\uc751\uc131 \ubc29\ubcbd",
      "upgrade_warp_stone_desc": "\uc804\ubc29\uc73c\ub85c <span class=\"highlight\">\uc21c\uac04\uc774\ub3d9</span>\ud558\uace0 <span class=\"highlight\">\ucd1d\ud0c4 \uc800\ud56d</span>\uc744 \uc5bb\uc2b5\ub2c8\ub2e4.",
      "upgrade_withering_whip_desc": "\ub300\uc0c1 \uc801\uad70\uc758 <span class=\"highlight\">\uc5f0\uc0ac \uc18d\ub3c4\ub97c \ub454\ud654</span>\ud558\uace0 <span class=\"highlight\">\ucd1d\ud0c4 \uc800\ud56d</span>\uc744 \uac10\uc18c\uc2dc\ud0b5\ub2c8\ub2e4."
    }
  }
